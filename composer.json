{
    "name": "chrisreedio/filament-secure-meilisearch",
    "description": "Filament integration for Secure Meilisearch Package.",
    "keywords": [
        "chrisreedio",
        "laravel",
        "filament-secure-meilisearch"
    ],
    "homepage": "https://github.com/chrisreedio/filament-secure-meilisearch",
    "support": {
        "issues": "https://github.com/chrisreedio/filament-secure-meilisearch/issues",
        "source": "https://github.com/chrisreedio/filament-secure-meilisearch"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Reed",
            "email": "chris@reedtech.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.0",
        "orchestra/testbench": "^8.0|^9.0",
        "larastan/larastan": "^2.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "ChrisReedIO\\FilamentSecureMeilisearch\\": "src/",
            "ChrisReedIO\\FilamentSecureMeilisearch\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisReedIO\\FilamentSecureMeilisearch\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisReedIO\\FilamentSecureMeilisearch\\FilamentSecureMeilisearchServiceProvider"
            ],
            "aliases": {
                "FilamentSecureMeilisearch": "ChrisReedIO\\FilamentSecureMeilisearch\\Facades\\FilamentSecureMeilisearch"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
